import os

def clean_file_extensions(folder_path):
    for filename in os.listdir(folder_path):
        if filename.endswith(".png.png"):
            new_name = filename.replace(".png.png", ".png")
            old_path = os.path.join(folder_path, filename)
            new_path = os.path.join(folder_path, new_name)
            os.rename(old_path, new_path)
            print(f"Renamed: {filename} to {new_name}")

if __name__ == "__main__":
    folder_path = r'H:\Switch Mod Master\Repositories\XGC_Pokedexs\Dexs\Sprite Image Dexs\Shiny Sprite Dex'
    clean_file_extensions(folder_path)
